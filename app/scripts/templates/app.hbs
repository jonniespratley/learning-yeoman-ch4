<div class="jumbotron hero-unit">
	<h1>{{ feature.title }}</h1>
	<img src="{{ feature.image}}" class="img-"/>
	<p class="lead">
		{{feature.body}}
	</p>
</div>
{{#each features}}
<div class="media post">
	<img class="media-object pull-left" src="{{image}}" alt="{{title}}"/>
	<h4>{{title}}</h4>
	<p>
		{{body}}
	</p>
</div>
{{/each}} 